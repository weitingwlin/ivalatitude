valueParse <-function(mod)  {
a <-   mod$part$indfract$Adj.R.square
#  showvarparts(3)
A <- a[1] + a[4] + a[6] + a[7] # a + d + f + g
B <- a[2] + a[4] + a[5] + a[7] # b + d + e + g
C <- a[3] + a[5] + a[6] + a[7] # c + e + f + g
AB <- a[4] + a[7] # d + g
AC <- a[6] + a[7] # f + g
BC <- a[5] + a[7] # e + g
ABC <- a[7]
res <- a[8]
out <- list("A" = A, "B" = B, "C" = C,
"AB" = AB, "AC" = AC, "BC"= BC,
"pureA"= a[1],"pureB"= a[2],"pureC"= a[3],
"res" = res)
return(out)
}
